    "build": "esbuild --bundle src/index.tsx --format=esm --outfile=dist/_worker.js",
    "deploy": "wrangler pages publish dist"
  },
  "dependencies": {
    "hono": "^3.6.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230922.0",
    "npm-run-all": "^4.1.5",
    "wrangler": "^3.9.1"
  }
}
